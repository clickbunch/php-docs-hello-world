function saveLogs(data) {
    if(data) {
        var tId = $.cookie('TicketId'), mode = (objCommon.isLive ? 'live' : 'dev'), logdata = {ref: mode + '_common_' +"web_" + jscommon_v,data: data, ua: navigator.userAgent, url: location.pathname + location.search};
