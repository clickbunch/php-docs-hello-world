        //console.log('grx', type, _grxDimension);
        /*if(objUser.checkPermission(['subscribed', 'expired_subscription', 'cancelled_subscription']) && (type == 'event' || type == 'page_view') && !window.objAnalytics.skipCleverTap && window.allowGdpr == 1) {
